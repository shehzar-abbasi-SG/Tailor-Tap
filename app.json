{
  "expo": {
    "name": "tailor-tap",
    "slug": "tailor-tap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#38D55B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.tailor-tap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#38D55B"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "fonts": "./assets/fonts"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your photos to upload screenshots"
        }
      ]
    ]
  }
}
