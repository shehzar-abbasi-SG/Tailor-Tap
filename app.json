{
  "expo": {
    "name": "tailor-tap",
    "slug": "tailor-tap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#38D55B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.tailor-tap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#38D55B"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.shehzarabbasisg.tailortap"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "fonts": "./assets/fonts",
      "eas": {
        "projectId": "6b648379-a58c-447b-8bca-e1dbbf54827e"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your photos to upload screenshots"
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6b648379-a58c-447b-8bca-e1dbbf54827e"
    }
  }
}
